<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.Tab?><?import javafx.scene.control.TabPane?><?import javafx.scene.layout.VBox?>
<VBox id="start-screen" fx:id="root" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
    minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="860.0"
   xmlns="http://javafx.com/javafx/8"
    xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label styleClass="setup-title" text="Game setup" VBox.vgrow="ALWAYS" />
        <TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab fx:id="gameSetup" closable="false" text="Setup" />
                <Tab fx:id="savedConfigs" closable="false" text="Presets" />
                <Tab fx:id="savedGames" closable="false" text="Saved games" />
            </tabs>
        </TabPane>
        <Button fx:id="start" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
            text="Start!" />
    </children>
</VBox>
